{
  "manifest_version": 2,
  "name": "Skroutz Vanced",
  "description": "Removes Sponsored Items and Adds Price Graphs.",
  "version": "0.0.3",
  "icons": {},
  "permissions": ["tabs", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://www.skroutz.gr/cart*"],
      "js": ["cart.js"]
    },
  {
    "matches": ["https://www.skroutz.gr/c/*"],
    "js": ["search.js","Chart.js","d3.v7.min.js"],
    "type": "module"
  },
  {
    "matches": ["https://www.skroutz.gr/s/*"],
    "js": ["product.js","Chart.js","d3.v7.min.js"],
    "type": "module"
  },{
    "matches": ["https://www.skroutz.gr/prosfores*"],
    "js": ["grafima.js","Chart.js","d3.v7.min.js"],
    "type": "module"
  },{
    "matches": ["https://www.skroutz.gr/plus-deals*"],
    "js": ["grafima.js","Chart.js","d3.v7.min.js"],
    "type": "module"
  }],
  "background": {
    "scripts": ["background.js"],
    "web_accessible_resources": [
      "images/*.png", "*.js"
    ]
  }
}