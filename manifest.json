{
  "manifest_version": 2,
  "name": "Skroutz Vanced",
  "description": "Removes Sponsored Items, Adds Price Graphs and Sorts Based on Product's Lowest Price.",
  "version": "0.0.5",
  "icons": {},
  "content_scripts": [
    {
      "matches": ["https://www.skroutz.gr/cart*"],
      "js": ["cart.js"]
    },
  {
    "matches": ["https://www.skroutz.gr/c/*"],
    "js": ["Chart.js","d3.v7.min.js","search.js","grafima.js"],
    "type": "module"
  },
  {
    "matches": ["https://www.skroutz.gr/s/*"],
    "js": ["product.js","Chart.js","d3.v7.min.js"],
    "type": "module"
  },{
    "matches": ["https://www.skroutz.gr/prosfores*","https://www.skroutz.gr/account/*","https://www.skroutz.gr/plus-deals*","https://www.skroutz.gr/shop*","https://www.skroutz.gr/m.*"],
    "js": ["Chart.js","d3.v7.min.js","grafima.js"],
    "type": "module"
  }],
  "background": {
    "scripts": ["background.js"],
    "pages": ["background.html"],
    "web_accessible_resources": [
      "images/*.png", "*.js"
    ]
  }
}